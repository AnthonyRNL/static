<h1>FOOOOOD TUUUNZ</h1>
<h4>searching for "<%=@word%>"</h4>
<%@tracks.each do |track|%>
<div class="row">
  <div class="large-4 columns">
    <h4><%=track.title%></h4>
    <form action="/searches/<%=track.id%>" method="GET">
      <button class="tiny round">Add</button>
      <input type="hidden" name="key" value="<%=@word%>">
      <input type="hidden" name="url" value="<%=track.uri%>">
      <input type="hidden" name="pic" value="https://cdn3.iconfinder.com/data/icons/kitchen-universe-1/140/5_cook_book-24.png">
    </form>
  </div>
  <div class="large-4 columns">
    <iframe width="100%" height="150" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=<%=track.uri%>&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe>
  </div>
</div>
<%end%>
